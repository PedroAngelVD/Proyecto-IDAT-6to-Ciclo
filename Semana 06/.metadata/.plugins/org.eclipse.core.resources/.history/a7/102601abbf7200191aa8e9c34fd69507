package com.demo.service;

import java.util.List;

import com.demo.model.Persona;

public interface IPersonaService {

	Persona registrar(Persona persona) throws Exception;
	
	Persona actualizar(Persona persona) throws Exception;
	
	Persona buscar(int id) throws Exception;
	
	List<Persona> listarTodos() throws Exception;

}
	

